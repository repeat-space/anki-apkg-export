{
  "name": "anki-apkg-export",
  "description": "Generate decks for Anki (spaced repetition software)",
  "version": "1.0.1",
  "main": "dist/index.js",
  "dependencies": {
    "jszip": "^3.1.2",
    "sha1": "^1.1.1",
    "sql.js": "^0.3.2"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "~6.6.0",
    "babel-eslint": "^5.0.0-beta9",
    "babel-preset-es2015": "~6.6.0",
    "eslint": "^1.10.3",
    "husky": "^0.11.3"
  },
  "author": "ewnd9 <ewndnine@gmail.com>",
  "keywords": [
    "anki", "spaced repetition software", "webpack"
  ],
  "license": "MIT",
  "preferGlobal": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/anki-apkg-export.git"
  },
  "scripts": {
    "build": "babel -d dist src",
    "build:watch": "babel -w -d dist src",
    "lint": "eslint src/*",
    "postpublish": "rm -rf dist",
    "prepublish": "npm run build",
    "prepush": "npm run lint && npm test",
    "test": "ava",
    "test:watch": "npm run test -- --watch"
  }
}
